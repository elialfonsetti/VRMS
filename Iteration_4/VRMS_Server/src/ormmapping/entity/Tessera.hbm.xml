<hibernate-mapping>
  <class lazy="false" name="entity.Tessera" table="Tessera">
    <id column="Codice" name="codice" type="string">
      <generator class="assigned">
			</generator>
    </id>
    <property column="Credito" lazy="false" name="credito" not-null="true" type="float"/>
    <property column="Attiva" lazy="false" length="1" name="attiva" not-null="true" type="boolean"/>
    <property column="Tipo_tariffa" lazy="false" length="255" name="tipo_tariffa" not-null="false" type="string"/>
  </class>
</hibernate-mapping>
